{
  "extends": ["airbnb", "prettier"],
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "no-console": "off",
    "import/first": 0,
    "func-names": 0,
    "no-underscore-dangle":0
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["./src"]
      }
    }
  }
}
